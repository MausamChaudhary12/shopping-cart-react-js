 state;\n      break;\n  }\n};\n\nexport default handleCart;\n","C:\\Users\\User\\Downloads\\New folder\\shopping-cart-react-js\\src\\components\\Footer.jsx",[],[],"C:\\Users\\User\\Downloads\\New folder\\shopping-cart-react-js\\src\\components\\About.js",[],[],"D:\\GitHub Project\\React\\shopping-cart-react-js\\src\\index.js",[],[],"D:\\GitHub Project\\React\\shopping-cart-react-js\\src\\App.js",[],[],"D:\\GitHub Project\\React\\shopping-cart-react-js\\src\\reportWebVitals.js",[],[],"D:\\GitHub Project\\React\\shopping-cart-react-js\\src\\redux\\store.js",[],[],"D:\\GitHub Project\\React\\shopping-cart-react-js\\src\\components\\Home.jsx",["167"],[],"D:\\GitHub Project\\React\\shopping-cart-react-js\\src\\components\\Navbar.jsx",[],[],"D:\\GitHub Project\\React\\shopping-cart-react-js\\src\\components\\About.js",[],[],"D:\\GitHub Project\\React\\shopping-cart-react-js\\src\\components\\Register.jsx",[],[],"D:\\GitHub Project\\React\\shopping-cart-react-js\\src\\components\\Cart.jsx",[],[],"D:\\GitHub Project\\React\\shopping-cart-react-js\\src\\components\\Login.jsx",[],[],"D:\\GitHub Project\\React\\shopping-cart-react-js\\src\\components\\Footer.jsx",[],[],"D:\\GitHub Project\\React\\shopping-cart-react-js\\src\\components\\Products.jsx",["168"],[],"D:\\GitHub Project\\React\\shopping-cart-react-js\\src\\redux\\reducers\\index.js",[],[],"D:\\GitHub Project\\React\\shopping-cart-react-js\\src\\redux\\action\\index.js",[],[],"D:\\GitHub Project\\React\\shopping-cart-react-js\\src\\redux\\reducers\\handleCart.js",["169","170","171"],[],{"ruleId":"172","severity":1,"message":"173","line":8,"column":9,"nodeType":"174","endLine":13,"endColumn":11},{"ruleId":"175","severity":1,"message":"176","line":31,"column":28,"nodeType":"177","endLine":31,"endColumn":33},{"ruleId":"175","severity":1,"message":"178","line":27,"column":6,"nodeType":"179","endLine":27,"endColumn":8,"suggestions":"180"},{"ruleId":"181","severity":1,"message":"182","line":24,"column":7,"nodeType":"183",